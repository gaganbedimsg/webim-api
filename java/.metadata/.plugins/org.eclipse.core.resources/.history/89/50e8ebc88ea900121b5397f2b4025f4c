package webim;

import org.json.JSONObject;

public class User {

	public String status;
	public String id;
	public String nick;
	public String show;

	public User(String id, String nick) {
		this.id = id;
		this.nick = nick;
	}

	public JSONObject toJson() {
		JSONObject json = new JSONObject();
		json.put("id", id);
		json.put("nick", nick);
		json.put("show", show);
		json.put("status", status);
		return json;
	}

}
