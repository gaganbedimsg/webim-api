package webim;

import java.util.Map;

public class Status {

	private String show;

	private String to;
	
	public Status(String to, String show, String status) {
		this.to = to;
		this.show = show;
	}

	public void feed(Map<String, String> data) {
		data.put("show", show);
		data.put("to", to);
	}
	
	public String toString() {
		return String.format("Status(to=%s, show=%s)", to, show);
	}

}
